{
  "name": "react-md-file",
  "version": "1.0.0",
  "description": "A component for rendering markdown files with React.",
  "license": "MIT",
  "author": "Gareth Williams (gazzwi86)",
  "homepage": "http://www.github.com/gazzwi86/react-md-file",
  "repository": {
    "type": "git",
    "url": "git://github.com/gazzwi86/react-md-file"
  },
  "bugs": {
    "url": "git://github.com/gazzwi86/react-md-file/issues"
  },
  "main": "./index.js",
  "scripts": {
    "start": "npm-run-all --parallel lint:watch test:watch serve",
    "build": "webpack --config ./webpack.prod.config.babel.js --define process.env.NODE_ENV='\"production\"' && npm pack",
    "lint": "esw webpack.config.* ./ --color --ext .js --ext .jsx",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "serve": "webpack-dev-server --config ./webpack.dev.config.babel.js --define process.env.NODE_ENV='\"development\"' --hot --open --content-base example",
    "release": "echo \"Please select an incredment eg. npm run release:patch\"",
    "release:major": "npm version major && npm run build && npm publish",
    "release:minor": "npm version minor && npm run build && npm publish",
    "release:patch": "npm version patch && npm run build && npm publish"
  },
  "jest": {
    "setupFiles": [
      "./jest-polyfills.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|png|gif|svg|ico|ttf|woff|woff2|eot|mp4|ico)$": "<rootDir>/fileMock.js"
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "markdown",
    "component"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.0.1",
    "file-loader": "^0.11.1",
    "html-to-react": "^1.2.7",
    "identity-obj-proxy": "^3.0.0",
    "img-loader": "^2.0.0",
    "jest-cli": "^19.0.2",
    "jest-fetch-mock": "^1.1.1",
    "npm-run-all": "^4.0.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.3",
    "sinon": "^2.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "marked": "^0.3.3",
    "prop-types": "^15.5.6",
    "react-html-parser": "^1.0.3",
    "react": "^15.5.3"
  }
}
