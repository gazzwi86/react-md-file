{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": ["transform-class-properties"],
  "env": {
    "test": {
      "presets": ["es2015"],
      "plugins": ["transform-class-properties"]
    },
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", {
          "mode": "wrap",
          "ignoreFilenames": ["node_modules"]
        }]
      ]
    }
  }
}
